<div class="row">
    <div class="bg-info" style="background-color: transparent; margin-bottom: 50px;">
      <div class="container">
        <div id="carousel-example-vertical" class="carousel vertical slide" style="padding-left: 229px; padding-right: 181px;">
          <div class="carousel-inner" role="listbox" data-interval="false">
            <!-- 1장이상인데 딱 떨어질 경우 -->
            <% if ((@showing_array_title.size/6) >= 1) && ((@showing_array_title.size%6) == 0) %>
              <div class="item active">
                  <p class="ticker-headline">
                    <% for a in (1..6) %>
                      <!--검색 결과값-->
                      <div class="row" style="margin-bottom: 14px;">
                        <!--노래 제목-->
                        <div class="col-xs-9">
                          <div class="output_song">
                            <div class="col-xs-10" style="padding-top: 0.6em;">
                            <%= @showing_array_title[@search_check_number] %>
                            </div>
                            <!--추가 버튼-->
                            <div class="col-xs-2" style="padding-left: 36px;">
                              <form action = "/whale/saving_song" >
                              <input type="hidden" name="search_sing" value="<%= params[:search_sing] %>">
                              <input type="image" src="/image/plus_icon.png" class="plus_image" 
                                     name="hidden_value" value="<%= @showing_array_id[@search_check_number] %>" data-toggle="modal" data-target="#plus_modal">
                              <button type="submit" class="plus_button"></button>
                              </form>
                            </div>
                          </div>
                        </div>
                        <!--가수명-->
                        <center>
                          <div class="col-xs-3">  
                            <div class="output_singer">
                            <%= @showing_array_singer[@search_check_number] %>
                            <% @search_check_number = @search_check_number + 1 %>
                            </div>
                          </div>
                        </center>
                      </div>
                    <% end %>
                  </p>
              </div>
              <% for a in (1..((@showing_array_title.size/6)-1)) %>
              <div class="item">
                <p class="ticker-headline">
                  <% for s in (1..6) %>
                    <div class="row" style="margin-bottom: 14px;">
                      <!--노래 제목-->
                      <div class="col-xs-9">
                        <div class="output_song">
                          <div class="col-xs-10" style="padding-top: 0.6em;">
                          <%= @showing_array_title[@search_check_number] %>
                          </div>
                          <!--추가 버튼-->
                          <div class="col-xs-2" style="padding-left: 36px;">
                            <form action = "/whale/saving_song" >
                            <input type="hidden" name="search_sing" value="<%= params[:search_sing] %>">
                            <input type="image" src="/image/plus_icon.png" class="plus_image" 
                                name="hidden_value" value="<%= @showing_array_id[@search_check_number] %>"data-toggle="modal" data-target="#plus_modal">
                            <button type="submit" class="plus_button"></button>
                            </form>
                          </div>
                        </div>
                      </div>
                      <!--가수명-->
                      <center>
                        <div class="col-xs-3">  
                          <div class="output_singer">
                          <%= @showing_array_singer[@search_check_number] %>
                          <% @search_check_number = @search_check_number + 1 %>
                          </div>
                        </div>
                      </center>
                    </div>
                  <% end %>
                </p>
              </div>
              <% end %>    
            <!-- 1장이상인데 딱떨어지지 않을 경우 -->  
            <% elsif ((@showing_array_title.size/6) >= 1) && ((@showing_array_title.size%6) != 0) %>
              <div class="item active">
                <p class="ticker-headline">
                  <% for a in (1..6) %>
                  <!--검색 결과값-->
                  <div class="row" style="margin-bottom: 14px;">
                    <!--노래 제목-->
                    <div class="col-xs-9">
                      <div class="output_song">
                        <div class="col-xs-10" style="padding-top: 0.6em;">
                        <%= @showing_array_title[@search_check_number] %>
                        </div>
                        <!--추가 버튼-->
                        <div class="col-xs-2" style="padding-left: 36px;">
                           <form action = "/whale/saving_song" >
                            <input type="hidden" name="search_sing" value="<%= params[:search_sing] %>">
                            <input type="image" src="/image/plus_icon.png" class="plus_image" 
                              name="hidden_value" value="<%= @showing_array_id[@search_check_number] %>"data-toggle="modal" data-target="#plus_modal">
                            <button type="submit" class="plus_button"></button>
                          </form>
                        </div>
                      </div>
                    </div>
                    <!--가수명-->
                    <center>
                      <div class="col-xs-3">  
                        <div class="output_singer">
                        <%= @showing_array_singer[@search_check_number] %>
                        <% @search_check_number = @search_check_number + 1 %>
                        </div>
                      </div>
                    </center>
                  </div>
                  <% end %>
                </p>
              </div>  
              <% for a in (1..(@showing_array_title.size/6)) %>
              <div class="item">
                          <p class="ticker-headline">
                            <% for s in (1..6) %>      
                            <div class="row" style="margin-bottom: 14px;">
                            
                            <!--노래 제목-->
                            <div class="col-xs-9">
                              <div class="output_song">
                                <div class="col-xs-10" style="padding-top: 0.6em;">
                                <%= @showing_array_title[@search_check_number] %>
                                </div>
                                <!--추가 버튼-->
                                <div class="col-xs-2" style="padding-left: 36px;">
                                <form action = "/whale/saving_song" >
                                <input type="hidden" name="search_sing" value="<%= params[:search_sing] %>">
                                <input type="image" src="/image/plus_icon.png" class="plus_image" 
                                  name="hidden_value" value="<%= @showing_array_id[@search_check_number] %>"data-toggle="modal" data-target="#plus_modal">
                                <button type="submit" class="plus_button"></button>
                                </form>
                                </div>
                              </div>
                            </div>
                            <!--가수명-->
                        <center>
                        <div class="col-xs-3">  
                          <div class="output_singer">
                            <%= @showing_array_singer[@search_check_number] %>
                            <% @search_check_number = @search_check_number + 1 %>
                          </div>
                        </div>
                        </center>
                      </div>
                            <% end %>
                          </p>
                        </div>    
              <% end %> 
            <!-- 1장 까지만 나올때 -->         
            <% else %>
              <div class="item active">
                      <p class="ticker-headline">
                        <% for a in (1..6) %>
                          <!--검색 결과값-->
                          <div class="row" style="margin-bottom: 14px;">
                            <!--노래 제목-->
                            <div class="col-xs-9">
                              <div class="output_song">
                                <div class="col-xs-10" style="padding-top: 0.6em;">
                                <%= @showing_array_title[@search_check_number] %>
                                </div>
                                <!--추가 버튼-->
                                <div class="col-xs-2" style="padding-left: 36px;">
                                   <form action = "/whale/saving_song" >
                                    <input type="hidden" name="search_sing" value="<%= params[:search_sing] %>">
                                    <input type="image" src="/image/plus_icon.png" class="plus_image" 
                                      name="hidden_value" value="<%= @showing_array_id[@search_check_number] %>"data-toggle="modal" data-target="#plus_modal">
                                    <button type="submit" class="plus_button"></button>
                                  </form>
                                </div>
                              </div>
                            </div>
                            <!--가수명-->
                            <center>
                            <div class="col-xs-3">  
                              <div class="output_singer">
                                <%= @showing_array_singer[@search_check_number] %>
                                <% @search_check_number = @search_check_number + 1 %>
                              </div>
                            </div>
                            </center>
                          </div>
                        <% end %>
                      </p>
                    </div>
            <% end %>
            <!-- Controls -->
            <a class="up carousel-control" href="#carousel-example-vertical" role="button" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-up" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
              </a>
            <a class="down carousel-control" href="#carousel-example-vertical" role="button" data-slide="next">
                <span class="glyphicon glyphicon-chevron-down" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
              </a>
          </div>
        </div>
      </div>
    </div>
  </div>